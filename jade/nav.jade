doctype html
html(lang='en')
  head
    include ../include/head
    title Navbar by Eden.
    link(href='css/style.css', rel='stylesheet')
  body
    .universe#nav-pg
        #nav
            .nav.clear
                .brand.float-left
                    a(href="index.html")
                        img.logo(alt="", src="images/icon.ico")
                        span brand
                ul.nav-options.float-left
                    li.active
                        a(href="#") option
                    li.dropdown option
                        ul.dropdown-content
                            li
                                a(href="#") dropdown option
                            li
                                a(href="#") dropdown option
                    li
                        a(href="#") option
                ul.nav-options.float-right
                    li.dropdown option
                        ul.dropdown-content
                            li
                                a(href="#") dropdown option
                            li
                                a(href="#") dropdown option
                    li
                        a(href="#") option
                    li
                        a(href="#") option
                
                .mob-nav.float-right
                    input.mob-toggle#mob-toggle(type="checkbox", name="mob-toggle")
                    label(for="mob-toggle")
                        .hamburger
                            span â˜°
                    ul.nav-options
                        li
                            a(href="#") option
                        li
                            a(href="#") option
                        li
                            a(href="#") option
                        
        
